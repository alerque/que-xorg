*ScrollbarPlacement: left
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.lcdfilter:  lcddefault
Xft.hinting:    true
Xft.hintstyle:  hintfull

URxvt*scrollBar_right: false
URxvt*scrollBar: false
URxvt*borderLess: false
URxvt*saveLines: 1000000
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: false
URxvt*scrollTtyKeypress: true
URxvt*internalBorder: 2
URxvt*lineSpace: 2

!Requires urxvt-font-size-git
URxvt.perl-ext-common: ...,font-size
URxvt.keysym.M-Up:     perl:font-size:increase
URxvt.keysym.M-Down:   perl:font-size:decrease
!URxvt.keysym.C-S-Up:   perl:font-size:incglobal
!URxvt.keysym.C-S-Down: perl:font-size:decglobal
URxvt.iso14755:        false
URxvt.iso14755_52:     false

!URxvt*inheritPixmap: true
!URxvt*tintColor: white
!URxvt*shading: 10

URxvt*font: xft:Liberation Mono for Powerline:size=10:antialias=true:hinting=true

*background: #000000
*foreground: #FFFFFF
*color0: #000000
*color1: #A80000
*color2: #00A800
*color3: #A85400
*color4: #0000A8
*color5: #A800A8
*color6: #00A8A8
*color7: #A8A8A8
*color8: #545054
*color9: #F85450
*color10: #50FC50
*color11: #F2FC50
*color12: #5054F8
*color13: #F854F8
*color14: #50FCF8
*color15: #F8FCF9
